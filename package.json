{
  "name": "hdg",
  "version": "1.0.0-beta.8",
  "description": "An API documentation generator CLI",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "lint": "prettier-standard index.js test/index.js src/{*,/**/*}.js",
    "lint:check": "prettier-standard index.js test/index.js src/{*,/**/*}.js --check",
    "pretest": "npm run lint:check",
    "test": "ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "cli",
    "node.js",
    "javascript"
  ],
  "author": "James George <jamesgeorge998001@gmail.com>",
  "dependencies": {
    "commander": "^6.1.0",
    "execa": "^4.0.3",
    "inquirer": "^7.3.3",
    "kleur": "^3.0.3",
    "node-banner": "^1.4.0",
    "ora": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "ava": "^2.4.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier-standard": "^16.4.1"
  }
}
